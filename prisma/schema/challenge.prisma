model Challenge21 {
  id String @id @default(uuid())

  user   User @relation(fields: [userId], references: [id])
  userId Int  @unique

  status      ChallengeStatus @default(NotStarted)
  checkInDays DateTime[]      @default([])

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@unique([id])
}
